	'BOLETO'														, ; //X5_DESCRI
	'BOLETO'														, ; //X5_DESCSPA
	'BOLETO'														} ) //X5_DESCENG

aAdd( aSX5, { ;
	'  '																	, ; //X5_FILIAL
	'_A'																	, ; //X5_TABELA
	'000002'																, ; //X5_CHAVE
	'NOTA FISCAL'																, ; //X5_DESCRI
	'NOTA FISCAL'																, ; //X5_DESCSPA
	'NOTA FISCAL'																} ) //X5_DESCENG

aAdd( aSX5, { ;
	'  '																	, ; //X5_FILIAL
	'_A'																	, ; //X5_TABELA
	'000003'																, ; //X5_CHAVE
	'RECIBO'																, ; //X5_DESCRI
	'RECIBO'																, ; //X5_DESCSPA
	'RECIBO'																} ) //X5_DESCENG

//
// Tabela 00
//
aAdd( aSX5, { ;
